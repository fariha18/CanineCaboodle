<?xml version="1.0" encoding="utf-8"?>
<androidx.realtivelayout.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <ImageButton
        android:id="@+id/breed1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:title="German Shepherd"
        android:src="@drawable/german_shepherd" />

    <ImageButton
        android:id="@+id/breed2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:title="Yorkshire Terrier"
        android:src="@drawable/yorkshire_terrier_" />

    <ImageButton
        android:id="@+id/breed3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:title="Golden Doodle"
        android:src="@drawable/golden_doodle" />

    <ImageButton
        android:id="@+id/breed4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:padding="20dp"
        android:title="Labrador"
        android:src="@drawable/labrador_" />
</androidx.realtivelayout.widget.RelativeLayout>